<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml" xmlns="http://www.w3.org/1999/html" xml:lang="en" lang="en">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8"/>
    <meta name="Robots" content="index,follow"/>
    <link rel="shortcut icon" type="image/ico" href="{{ STATIC_URL }}simulocean/img/favicon.ico"/>
    <title>{% block title %}Simulocean{% endblock %}</title>
    <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.10.1/jquery.min.js"></script>
    <script src="http://ajax.googleapis.com/ajax/libs/jqueryui/1.10.3/jquery-ui.min.js"></script>
    <script src="http://netdna.bootstrapcdn.com/twitter-bootstrap/2.3.2/js/bootstrap.min.js"></script>

    {% block local_js %}{% endblock %}
    <link href="http://netdna.bootstrapcdn.com/twitter-bootstrap/2.3.2/css/bootstrap-combined.min.css"
          rel="stylesheet">
    {#    <link href="//netdna.bootstrapcdn.com/twitter-bootstrap/2.3.2/css/bootstrap-responsive.min.css"#}
    {#          rel="stylesheet">#}
    <link rel="stylesheet" type="text/css"
          href="{{ STATIC_URL }}simulocean/css/index.css"/>
    {% block local_css %}{% endblock %}
</head>

{% load activetab_tags %}
{% url simulocean_home as home %}
{% url simulocean_about as about %}
{% url simulocean_services as services %}
{% url simulocean_tutorials as tutorials %}
{% url simulocean_software as software %}

{#{% url simulocean_license as license %}#}
{% url workflow_projectlist as projectlist %}
{% url simulocean_contact as contact %}
{% url simulocean_doc_detached as doc_detached %}

<body>
<div class="container">
    <div class="masthead">
        <a href="{{ home }}"><img style="padding-bottom: 1px;" src="{{ STATIC_URL }}simulocean/img/logo.jpg"/></a>

        <div style="float: right">
            {% if user.is_authenticated %}
                <img src="{{ STATIC_URL }}simulocean/img/user.png"/>
                <span>Welcome back, {{ user.username }}! </span>
                <a class="btn btn-primary btn-mini" href="{% url auth_logout %}" title="Sign out">
                    <i class="icon-off icon-white"></i><span> Sign Out</span>
                </a>

                <a class="btn btn-primary btn-mini" href="{{ projectlist }}">
                    <i class="icon-th-large icon-white"></i><span> My Projects</span>
                </a>
                {% url workflow_projectlist as projectlist %}
            {% else %}
                <a class="btn btn-primary btn-mini" href="{% url auth_login %}" title="sign in">
                    <i class="icon-user icon-white"></i><span> Sign In</span>
                </a>
                <a class="btn btn-primary btn-mini" href="{% url registration_register %}" title="Register">
                    <i class="icon-edit icon-white"></i><span> Register</span>
                </a>
                {#                <a href="{% url google-login %}">#}
                {#                    <button class="btn btn-primary btn-mini">#}
                {#                        <img style="height: 18px;" src="{{ STATIC_URL }}simulocean/img/logo_google.png"#}
                {#                             alt="log in with google"/>#}
                {#                        <span> Sign in with Google</span>#}
                {#                    </button>#}
                {##}
                {#                </a>#}
            {% endif %}
        </div>
        {#      <h3 class="muted">Simulocean</h3>#}
        <div class="navbar">
            <div class="navbar-inner">
                <div class="container">
                    <ul class="nav">
                        <li class="{% active request '^/$' %}"><a href="{{ home }}">Home</a></li>
                        <li class="{% active request '^/about/services/$' %}"><a href="{{ services }}">Services</a></li>
                        {#                        <li class="{% active request '^/about/partners/$' %}"><a href="{{ partners }}">Partners</a></li>#}
                        <li class="{% active request '^/about/software/$' %}"><a href="{{ software }}">Software</a>
                        </li>
                        <li class="{% active request '^/about/contact/$' %}"><a href="{{ contact }}">Forum</a></li>
                        {#                        <li class="{% active request '^/doc/detached/$' %}"><a href="{{ doc_detached }}">Documents</a>#}
                        </li>
                        <li class="{% active request '^/about/tutorials/$' %}"><a href="{{ tutorials }}">Tutorials</a>
                        </li>
                        <li class="{% active request '^/about/$' %}"><a href="{{ about }}">About</a></li>
                    </ul>
                </div>
            </div>
        </div>
        <!-- /.navbar -->
    </div>
    <!-- Jumbotron -->
    <div>
        {% block marketing %}
        {% endblock %}
    </div>

    <!-- Jumbotron -->
    {% block jumbotron %}
    {% endblock %}
    <!-- Example row of columns -->
    <div class="well-large">
        {% block components %}
        {% endblock %}
    </div>
    <hr>

    <div class="foot" style="text-align: center">

        {% block footer %}
            <a href="http://coastalhazards.org" target="_blank"
               title="Coastal Hazards Research Collaboratory">
                <img style="height:35px" src="{{ STATIC_URL }}simulocean/img/logo_charcoal.png" alt="charcoal"/></a>
            <a href="http://www.cct.lsu.edu" target="_blank"
               title="Center for Computation and Technology at Louisiana State University">
                <img style="height:35px" src="{{ STATIC_URL }}simulocean/img/logo_cct.png" alt="lsu-cct"/></a>
            <a href="http://www.loni.org" target="_blank" title="Louisiana Optical Network Initiative">
                <img src="{{ STATIC_URL }}simulocean/img/logo_loni.png" alt="loni" height="35"/></a>
            <a href="http://www.ngchc.org" target="_blank" title="Northern Gulf Coastal Hazards Collaboratory">
                <img style="height:35px" src="{{ STATIC_URL }}simulocean/img/logo_ngchc.png" alt="ngchc"/></a>
        {% endblock %}
        <br/><br/>

        <div class="container">
            <p class="muted">SIMULOCEAN has been supported by</p>

            <p><a href="http://www.nsf.gov/" target="_blank" title="NSF"><img style="height:50px;"
                                                                              src="{{ STATIC_URL }}simulocean/img/logo_nsf.jpg"
                                                                              alt="NSF"/> the National Science
                Foundation</a> (<a href="http://www.nsf.gov/awardsearch/showAward?AWD_ID=1010640"
                                   target="_blank" title="NSF">Award #1010640</a>)
            </p>
        </div>
    </div>
    {#        <p>&copy; Company 2013</p>#}
</div>
</body>
</html>
