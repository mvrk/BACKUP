{% block page_content %}
    <form class="actionform" action="{% url datafactory_datarequestedit datarequest_id %}" method="post">
        {% csrf_token %}
        <table>
            {{ datarequest_form.as_table }}
            <tr>
                <th></th>
                <td>
                    <input class="actionbut" type="submit" value="Save"></td>
            </tr>

        </table>
    </form>
{% endblock %}
{% block page_hints %}
<h4>Datarequest edit</h4>
<hr/>
{% endblock %}

{% block hint_title %}
Data request edit
{% endblock %}
{% block hint_content %}
Edit your data requesting parameters here.
{% endblock %}